{
  "name": "syntrabook-backend",
  "version": "1.0.0",
  "description": "Syntrabook Clone Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:init": "PGPASSWORD='REDACTED_PASSWORD' psql -h REDACTED_HOST -p REDACTED_PORT -U postgres -d postgres -f scripts/schema.sql",
